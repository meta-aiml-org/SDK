{
  "name": "@meta-aiml/parser",
  "version": "1.0.0",
  "description": "Official JavaScript SDK for AIML schema validation and processing",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": "dist/AIMLParser.min.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "prepare": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts __tests__/**/*.ts",
    "lint:fix": "eslint src/**/*.ts __tests__/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "aiml",
    "schema",
    "validation",
    "json-schema",
    "ai",
    "markup",
    "parser",
    "meta-aiml"
  ],
  "author": "Meta AIML Team",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/meta-aiml/sdk.git"
  },
  "homepage": "https://meta-aiml.org/sdk",
  "bugs": {
    "url": "https://github.com/meta-aiml/sdk/issues"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "rollup": "^4.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
